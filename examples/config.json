[
    {
        "schedule": "0/2 * * * * *",
        "command": "bash",
        "args": [
            "-c",
            "echo Hello $NAME"            
        ]
    },
    {
        "schedule": "0/5 * * * * *",
        "command": "curl",
        "args": [
            "-s",
            "-S",
            "-X",
            "GET",
            "https://example.com"            
        ]
    },
    {
        "schedule": "0 0 3 ? * *",
        "command": "/usr/bin/restic",
        "args": [
            "-r",
            "$REPO__FILE",
            "-p",
            "/configs/passwd",
            "forget",
            "--keep-daily",
            "7"
        ]
    }
]